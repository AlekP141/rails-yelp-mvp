<div class="container mt-5">
  <h1>Restaurant List</h1>
  <ul class="list-group">
    <% @restaurants.each do |restaurant| %>
    <li class="list-group-item"><%= link_to restaurant.name, restaurant_path(restaurant) %></li>
    <% end %>
  </ul>
  <br>
  <%= link_to "Add new Restaurant", new_restaurant_path %>
</div>
