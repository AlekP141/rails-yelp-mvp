<div class="container mt-5">
  <h1>New review</h1>
  <%= simple_form_for [@restaurant, @review] do |f| %>
    <%= f.input :content %>
    <%= f.input :rating, collection:(0..5) %>
    <%= f.submit "Submit", class: "btn btn-primary" %>
  <% end %>
  <br>
  <%= link_to "< Back", restaurants_path %>
</div>
